@model PetBook.Models.MedicalFile

@{
    ViewData["Title"] = "Add a Medical File";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<h1>AddMedicalFile</h1>

<h4>MedicalFile</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddMedicalFile">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="MedicalFileId" class="control-label"></label>
                <input asp-for="MedicalFileId" class="form-control" />
                <span asp-validation-for="MedicalFileId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PetId" class="control-label"></label>
                <select asp-for="PetId" class="form-control" asp-items="ViewBag.PetId"></select>
            </div>
            <div class="form-group">
                <label asp-for="VaccinationId" class="control-label"></label>
                <select asp-for="VaccinationId" class="form-control" asp-items="ViewBag.VaccinationId"></select>
            </div>
            <div class="form-group">
                <label asp-for="MedicationId" class="control-label"></label>
                <select asp-for="MedicationId" class="form-control" asp-items="ViewBag.MedicationId"></select>
            </div>
            <div class="form-group">
                <label asp-for="ProcedureId" class="control-label"></label>
                <select asp-for="ProcedureId" class="form-control" asp-items="ViewBag.ProcedureId"></select>
            </div>
            <div class="form-group">
                <label asp-for="AppointmentId" class="control-label"></label>
                <select asp-for="AppointmentId" class="form-control" asp-items="ViewBag.AppointmentId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

